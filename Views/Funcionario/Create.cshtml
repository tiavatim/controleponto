<script type="text/javascript">

        $("#btnCEP").on("click", function () {

        var cepcampo = $("#CEP").val();

         var data = JSON.stringify({ cep: cepcampo });
        $.ajax({
            url: '@Url.Action("BuscarCEP", "Funcionario")' + '?cep=' + cepcampo.toString(),
            type: 'POST',
            data: cepcampo.toString(),
            success: function (data) {
                $("#Logradouro").val(data.bag.logradouro);
                $("#Cidade").val(data.bag.localidade);
                $("#Estado").val(data.bag.uf);


               console.log(data)

            },
            error: function (error) {

            }
        })
         });


</script>


<script type="text/javascript">

    $("#btnCreate").on("click", function () {
        alert("clicou")

        var horaInicio = parseInt($('#HoraInicio').val());
        alert("hora inicial = " + horaInicio)
        var horaFim = parseInt($('#HoraFim').val());
        alert("horaFim = " + horaFim)
        var tempoDescanso = parseInt($('#TempodeDescanso').val());
        alert("tempoDescanso = " + tempoDescanso);

        var cargaHoraria = horaFim - horaInicio - tempoDescanso;
        alert("cargaHoraria " + cargaHoraria);

        $('#CargaHoraria').val(cargaHoraria);


        if (parseInt(cargaHoraria) > 10) {
            alert("Passou de 10")
        }


    });

</script>


